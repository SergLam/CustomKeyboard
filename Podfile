def regular_pods
  
  # Networking
   pod 'Moya', '~> 13.0.1'
   pod 'ReachabilitySwift', '~> 4.3.1'
   
end

target 'HostingApp' do
  regular_pods
end

target 'Keyboard' do
   regular_pods
end

target 'KeyboardFramework' do
   regular_pods
end
